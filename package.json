{
  "name": "BaseRN",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "devices": "xcrun simctl list devices",
    "android ram": "adb shell dumpsys meminfo",
    "xcode": "open ios/BaseRN.xcworkspace",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "pod": "cd ios && pod install && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "ios-run-develop": "ENVFILE=.env.dev react-native run-ios",
    "ios-run-product": "ENVFILE=.env.prod react-native run-ios",
    "ios-run-staging": "ENVFILE=.env.stage react-native run-ios",
    "android-run-develop": "ENVFILE=.env.dev react-native run-android",
    "android-run-product": "ENVFILE=.env.prod react-native run-android",
    "android-run-staging": "ENVFILE=.env.stage react-native run-android"
  },
  "dependencies": {
